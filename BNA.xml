<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<RECORD>
		<FIELD ID="1" xsi:type="CharFixed"  LENGTH="10"/>
		<FIELD ID="2" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="3" xsi:type="CharFixed"  LENGTH="45"/>
		<FIELD ID="4" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="5" xsi:type="CharFixed"  LENGTH="4"/>
		<FIELD ID="6" xsi:type="CharTerm" TERMINATOR="\r\n" LENGTH="2"/>

	</RECORD>
	<ROW>
		<COLUMN SOURCE="1"  NAME="[CASNumber]"  xsi:type="SQLCHAR" />
		<COLUMN SOURCE="2"  NAME="[LanguageCode]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="3"  NAME="[SubstanceName]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="4"  NAME="[YearOfReference]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="5"  NAME="[ReferenceCode]" xsi:type="SQLCHAR" />
	</ROW>

</BCPFORMAT>