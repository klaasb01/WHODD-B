<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<RECORD>
		<FIELD ID="1" xsi:type="CharFixed"  LENGTH="6"/>
		<FIELD ID="2" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="3" xsi:type="CharFixed"  LENGTH="3"/>
		<FIELD ID="4" xsi:type="CharFixed"  LENGTH="1"/>
		<FIELD ID="5" xsi:type="CharFixed"  LENGTH="10"/>
		<FIELD ID="6" xsi:type="CharTerm" TERMINATOR="\r\n" LENGTH="2"/>

	</RECORD>
	<ROW>
		<COLUMN SOURCE="1"  NAME="[DrugRecordNumber]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="2"  NAME="[SequenceNumber1]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="3"  NAME="[SequenceNumber2]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="4"  NAME="[CheckDigit]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="5"  NAME="[CASNumber]" xsi:type="SQLCHAR" />
		

	</ROW>
</BCPFORMAT>