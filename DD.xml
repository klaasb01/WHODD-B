<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<RECORD>
		<FIELD ID="1" xsi:type="CharFixed"  LENGTH="6"/>
		<FIELD ID="2" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="3" xsi:type="CharFixed"  LENGTH="3"/>
		<FIELD ID="4" xsi:type="CharFixed"  LENGTH="1"/>
		<FIELD ID="5" xsi:type="CharFixed"  LENGTH="1"/>
		<FIELD ID="6" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="7" xsi:type="CharFixed"  LENGTH="4"/>
		<FIELD ID="8" xsi:type="CharFixed"  LENGTH="5"/>
		<FIELD ID="9" xsi:type="CharFixed"  LENGTH="2"/>
		<FIELD ID="10" xsi:type="CharFixed" LENGTH="1"/>
		<FIELD ID="11" xsi:type="CharFixed" LENGTH="3"/>
		<FIELD ID="12" xsi:type="CharFixed" LENGTH="45"/>
		<FIELD ID="13" xsi:type="CharTerm" TERMINATOR="\r\n" LENGTH="2"/>

	</RECORD>
	<ROW>
		<COLUMN SOURCE="1"  NAME="[DrugRecordNumber]"  xsi:type="SQLCHAR" />
		<COLUMN SOURCE="2"  NAME="[SequenceNumber1]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="3"  NAME="[SequenceNumber2]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="4"  NAME="[CheckDigit]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="5"  NAME="[Designation]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="6"  NAME="[YearOfReference]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="7"  NAME="[ReferenceCode]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="8"  NAME="[CompanyCode]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="9"  NAME="[NumberOfIngredients]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="10" NAME="[Salt_Ester_Code]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="11" NAME="[YearQuarter]" xsi:type="SQLCHAR" />
		<COLUMN SOURCE="12" NAME="[DrugName]" xsi:type="SQLCHAR" />
		
		
	</ROW>
</BCPFORMAT>